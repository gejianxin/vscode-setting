{
    "sync.gist": "36a52952fae4d19c1261567f7da39e15",
    "kite.showWelcomeNotificationOnStartup": false,
    "prettier.tabWidth": 4,
    "latex-workshop.bibtex-format.tab": "4 spaces",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ],
    "window.openWithoutArgumentsInNewWindow": "off",
    "window.openFoldersInNewWindow": "off",
    "window.zoomLevel": 1,
    "git.ignoreWindowsGit27Warning": true,
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.visualstar": true,
    "vim.ignorecase": false,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [{
        "before": ["k", "j"],
        "after": ["<Esc>"]
    }],
    "vim.leader": " ",
    "vim.visualModeKeyBindingsNonRecursive": [{
            "before": ["%"],
            "commands": ["extension.matchitJumpItems"]
        },
        {
            "before": ["<leader>", "s", "i"],
            "commands": ["extension.matchitSelectItems"]
        },
        {
            "before": ["<leader>", "d", "i"],
            "commands": ["extension.matchitDeleteItems"]
        },
        {
            "before": ["<leader>", "c", "l"],
            "commands": ["editor.action.commentLine"]
        },
        {
            "before": ["<leader>", "e", "n"],
            "commands": ["workbench.action.nextEditor"]
        },
        {
            "before": ["<leader>", "e", "p"],
            "commands": ["workbench.action.previousEditor"]
        },
        {
            "before": ["<leader>", "f", "f"],
            "commands": ["workbench.action.findInFiles"]
        },
        {
            "before": ["<leader>", "s", "s"],
            "commands": ["actions.find"]
        },
        {
            "before": ["<leader>", "r", "r"],
            "commands": ["workbench.action.openRecent"]
        },
        {
            "before": ["<leader>", "k", "k"],
            "commands": ["workbench.action.quickOpen"]
        },
        {
            "before": ["<leader>", "i", "i"],
            "commands": ["workbench.action.gotoSymbol"]
        },
        {
            "before": ["<leader>", "x", "x"],
            "commands": ["editor.action.smartSelect.expand"]
        },
        {
            "before": ["<leader>", "z", "z"],
            "commands": ["editor.action.smartSelect.shrink"]
        },
        {
            "before": ["<leader>", "e", "b"],
            "commands": ["code-runner.run"]
        },
        {
            "before": ["<leader>", "e", "e"],
            "commands": ["code-runner.runCustomCommand"]
        },
        {
            "before": ["<leader>", "e", "l"],
            "commands": ["code-runner.runByLanguage"]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [{
            "before": ["%"],
            "commands": ["extension.matchitJumpItems"]
        },
        {
            "before": ["<leader>", "s", "i"],
            "commands": ["extension.matchitSelectItems"]
        },
        {
            "before": ["<leader>", "d", "i"],
            "commands": ["extension.matchitDeleteItems"]
        },
        {
            "before": ["<leader>", "c", "l"],
            "commands": ["editor.action.commentLine"]
        },
        {
            "before": ["<leader>", "e", "n"],
            "commands": ["workbench.action.nextEditor"]
        },
        {
            "before": ["<leader>", "e", "p"],
            "commands": ["workbench.action.previousEditor"]
        },
        {
            "before": ["<leader>", "f", "f"],
            "commands": ["workbench.action.findInFiles"]
        },
        {
            "before": ["<leader>", "s", "s"],
            "commands": ["actions.find"]
        },
        {
            "before": ["<leader>", "f", "p"],
            "commands": ["workbench.action.files.copyPathOfActiveFile"]
        },
        {
            "before": ["<leader>", "f", "n"],
            "commands": ["copyRelativeFilePath"]
        },
        {
            "before": ["<leader>", "t", "p"],
            "commands": ["workbench.action.togglePanel"]
        },
        {
            "before": ["<leader>", "t", "z"],
            "commands": ["workbench.action.toggleZenMode"]
        },
        {
            "before": ["<leader>", "t", "a"],
            "commands": ["workbench.action.toggleActivityBarVisibility"]
        },
        {
            "before": ["<leader>", "t", "s"],
            "commands": ["workbench.action.toggleSidebarVisibility"]
        },
        {
            "before": ["<leader>", "x", "m"],
            "commands": ["workbench.action.showCommands"]
        },
        {
            "before": ["<leader>", "x", "s"],
            "commands": ["workbench.action.files.save"]
        },
        {
            "before": ["<leader>", "x", "f"],
            "commands": ["workbench.action.files.openFile"]
        },
        {
            "before": ["<leader>", "x", "k"],
            "commands": ["workbench.action.closeActiveEditor"]
        },
        {
            "before": ["<leader>", "r", "r"],
            "commands": ["workbench.action.openRecent"]
        },
        {
            "before": ["<leader>", "k", "k"],
            "commands": ["workbench.action.quickOpen"]
        },
        {
            "before": ["<leader>", "i", "i"],
            "commands": ["workbench.action.gotoSymbol"]
        },
        {
            "before": ["<leader>", "x", "x"],
            "commands": ["editor.action.smartSelect.expand"]
        },
        {
            "before": ["<leader>", "z", "z"],
            "commands": ["editor.action.smartSelect.shrink"]
        },
        {
